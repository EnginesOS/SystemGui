<div class="col-sm-12 top-gap app_installs">
  <h2 class=''><i class='fa fa-desktop'></i> Edit display properties for <%= @software.engine_name %></h2>
  <%= form_for(@software, url: display_path(@software) ) do |f| %>
    <%= render partial: 'shared/form/errors', locals: {f: f, label: 'display properties'} %>
    <%= render partial: 'form_fields', locals: {f: f} %>
    <%= render partial: 'icon_field', locals: {f: f} %>
    <%= render partial: 'shared/form/cancel_and_submit_buttons', locals: {f: f, cancel_path: control_panel_path} %>
  <% end %>
</div>

