<div class="col-sm-12 top-gap">
  <h3 class=''><i class='fa fa-sitemap'></i> Edit resources for <%= @resources_properties.application_name %></h3>
  <%= form_for @resources_properties, 
  					url: resources_properties_path(
					application_name: @resources_properties.application_name),
					method: :patch do |f| %>
    <%= render partial: 'shared/form/errors', locals: {f: f} %>
    <%= render partial: 'form_fields', locals: {f: f} %>
    <%= render partial: 'shared/form/cancel_and_submit_buttons', locals: {f: f, cancel_path: control_panel_path} %>
  <% end %>
</div>